---
import Layout from "../layouts/Layout.astro";
import AboutLayout from "../layouts/AboutLayout.astro";
import * as i18n from "../i18n/en.md";
import Content from '../html/about.html'

const title = [i18n.frontmatter.aboutDatabase, i18n.frontmatter.lawForPalestine].join(" - ");
---

<Layout {title} description={i18n.frontmatter.description}>
  <AboutLayout back={i18n.frontmatter.back}>
    <Content />
  </AboutLayout>
</Layout>
